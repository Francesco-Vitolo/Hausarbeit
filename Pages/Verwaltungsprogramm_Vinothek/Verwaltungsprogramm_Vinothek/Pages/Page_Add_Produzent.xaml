<Page x:Class="Verwaltungsprogramm_Vinothek.Pages.Page_Add_Produzent"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Verwaltungsprogramm_Vinothek.Pages" 
      xmlns:local1="clr-namespace:Verwaltungsprogramm_Vinothek.User_Controls"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page_Add_Produzent">
    <Grid>
        <local1:Uc_Default/>
        <Button x:Name="GoBack" Background="#FF6990B8" Content="🢢" Grid.Column="1"  Style="{StaticResource Button_Titlebar}" Click="Button_Previous_Click"/>
        <local1:Uc_Produzent x:Name="felder"/>
        <Button  Content="Save" FontSize="24" Click="Button_Click_Save" VerticalAlignment="Bottom" Margin="25" Width="75"/>
    </Grid>
</Page>
