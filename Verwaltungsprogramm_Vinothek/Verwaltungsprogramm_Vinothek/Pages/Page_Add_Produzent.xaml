        <local:Uc_Produzent x:Name="felder"/>
        <Button x:Name="saveChanges" Click="Button_Click_Save" VerticalAlignment="Bottom" Style="{StaticResource Button_Style2}">
            <Image Source="..\Pictures\save.png"/>
        </Button>
    </StackPanel>
</Page>
