<Page x:Class="Verwaltungsprogramm_Vinothek.Pages.Page_Add_Produzent"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Verwaltungsprogramm_Vinothek.User_Controls"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page_Add_Produzent">
    <Grid>
        <Grid.LayoutTransform>
            <ScaleTransform ScaleX="1.5" ScaleY="1.5"/>
        </Grid.LayoutTransform>
        <local:Uc_Produzent x:Name="felder" VerticalAlignment="Center"/>
        <Button x:Name="saveChanges" Click="Button_Click_Save" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="100">
            <Image Source="..\Images\save.png"/>
        </Button>
    </Grid>
</Page>
