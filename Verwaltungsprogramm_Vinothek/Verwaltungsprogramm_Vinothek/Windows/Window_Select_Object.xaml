<Window x:Class="Verwaltungsprogramm_Vinothek.Window_Select_Object"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Verwaltungsprogramm_Vinothek.User_Controls"
        mc:Ignorable="d"
        Title="Window_Select_Object" Height="900" Width="1600" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <local:Uc_Titlebar/>
        <TextBox Grid.Column="1" Grid.Row="1" TextAlignment="Center" VerticalContentAlignment="Center" Margin="10"  Name="tbSearch" KeyUp="tbSearch_KeyUp"/>
        <DataGrid Grid.Row="2" Grid.Column="1"  Name="data" MouseDoubleClick="SelectItem_DoubleClick"/>
    </Grid>
</Window>

